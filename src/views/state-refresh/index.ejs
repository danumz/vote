    <script>
        $(document).ready(function () {
            $('#candidate').select2({
                placeholder: "Suggested Candidate",
                allowClear: true
            });
        });
        const voteBtn = document.getElementById('voteBtn');
        const voteForm = document.getElementById('voteForm');

        voteBtn.addEventListener('click', () => {
            voteForm.style.display = 'block';
            voteBtn.style.display = 'none';
        });

        voteForm.addEventListener('submit', (e) => {
            e.preventDefault();

            const name = document.getElementById('name').value.trim();
            const house = document.getElementById('house').value.trim();
            const candidate = document.getElementById('candidate').value.trim();

            const formData = {
                name,
                house,
                candidate
            };

            fetch('https://vote-beta-ten.vercel.app/vote', {
                method: 'POST',
                body: JSON.stringify(formData),
                headers: {
                    'Content-Type': 'application/json'
                }
            })
                .then(response => response.json())
                .then(data => {
                    console.log('Success:', data);
                    alert('Thank you for voting!');
                    voteForm.reset();
                    voteForm.style.display = 'none';
                    voteBtn.style.display = 'inline-block';
                })
                .catch(error => {
                    console.error('Error:', error);
                    alert('There was an error submitting your vote.');
                });

        });
    </script>

